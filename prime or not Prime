   namespace primenumber
{
   public class program
   {
       static void Main(string[]args)
       {
           Console.WriteLine("Enter the number prime or notPrime");
           int n = int.Parse(Console.ReadLine());
           bool isprime = true;
           for (int i=2; i<n; i++)
           {
               if (n%1==0)
               {
                   isprime = false;
                   break;
               }
           }
       }
   }
}
